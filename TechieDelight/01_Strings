import datetime

class SystemFiles():
    def get_files():
        files = list()
        return files

class Utils():
    def get_last_month(self):
        today = datetime.date.today()
        first = today.replace(day=1)
        last_month = first - datetime.timedelta(days=1)
        return last_month
    
    def get_date_range(self):
        pass

    